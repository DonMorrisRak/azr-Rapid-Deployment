{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageAccountType": {
      "allowedValues": [
        "Standard_LRS",
        "Standard_GRS",
        "Standard_RAGRS",
        "Standard_ZRS",
        "Premium_LRS"
      ],
      "defaultValue": "Standard_LRS",
      "metadata": {
        "description": "The type of the Storage Account created"
      },
      "type": "string"
    },
    "storageAccountCount": {
      "type": "int",
      "minValue": 1,
      "maxValue": 100,
      "defaultValue": 1,
      "metadata": {
        "description": "Number of storage accounts to be created"
      }
    },
    "startIndex": {
      "type": "int",
      "metadata": {
        "description": "The Starting Index for the storage account to create (will be used to append a number at the end)"
      },
      "minValue": 1,
      "maxValue": 99,
      "defaultValue": 1
    },
    "storageAccountSuffix": {
      "type": "string",
      "minLength": 3,
      "maxLength": 15,
      "metadata": {
        "description": "The suffix name to use for this storage account"
      },
      "defaultValue": "Other"
    },
    "blobEncryptionEnabled": {
      "type": "bool",
      "defaultValue": false,
      "metadata": {
        "description": "Enable or disable Blob encryption at Rest."
      }
    },
    "accessTier": {
      "type": "string",
      "defaultValue": "Hot",
      "allowedValues": [
        "Cool",
        "Hot"
      ]
    },
    "environment": {
      "type": "string",
      "allowedValues": [
        "Production",
        "Staging",
        "Test",
        "Development",
        "Q/A",
        "Bastion",
        "Other"
      ],
      "metadata": {
        "description": "The Environment type we are building"
      },
      "defaultValue": "Production"
    },
    "buildDate": {
      "type": "string",
      "metadata": {
        "description": "The date of this build"
      },
      "defaultValue": "01/01/2999"
    },
    "buildBy": {
      "type": "string",
      "metadata": {
        "description": "The name of the person who built this environment"
      },
      "defaultValue": "First Last"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Storage/storageAccounts",
      "name": "[concat(substring(uniqueString(subscription().id, resourceGroup().id, string(copyIndex(parameters('startIndex')))) ,0,6), tolower(parameters('storageAccountSuffix')), copyindex(parameters('startIndex')))]",
      //"apiVersion": "2016-12-01",
      "apiVersion": "2017-06-01",
      "location": "[resourceGroup().location]",
      "copy": {
        "name": "storageLoop",
        "count": "[parameters('storageAccountCount')]"
      },
      "sku": {
        "name": "[parameters('storageAccountType')]"
      },
      "kind": "BlobStorage",
      "tags": {
        "displayName": "StorageAccount",
        "Environment": "[parameters('environment')]",
        "BuildDate": "[parameters('buildDate')]",
        "BuildBy": "[parameters('buildBy')]"
      },
      "properties": {
        "encryption": {
          "keySource": "Microsoft.Storage",
          "services": {
            "blob": {
              "enabled": "[parameters('blobEncryptionEnabled')]"
            }
          }
        },
        "accessTier": "[parameters('accessTier')]"
      }
    }
  ],
